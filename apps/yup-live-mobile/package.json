{
  "name": "yup-live-mobile",
  "version": "0.0.1",
  "private": true,
  "description": "Ionic Vue Yup Live Mobile",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src&&prettier . -w -u",
    "build": "vue-tsc --noEmit && vite build",
    "android": "ionic cap run android -l --external --host=4875.flashsoft.eu --port=4875 --livereload-url=https://4875.flashsoft.eu",
    "android-build": "yarn build && ionic capacitor sync android --prod --no-build",
    "android-open": "ionic capacitor open android",
    "android-res": "cordova-res android --skip-config --copy",
    "android-ver": "deno run -A ./scripts/update-version.ts",
    "android-pub": "deno run -A ./scripts/publish-preview.ts",
    "android-res-new": "npx capacitor-assets generate --android",
    "upgrade": "yarn-upgrade-all"
  },
  "dependencies": {
    "@capacitor-firebase/messaging": "^5.1.0",
    "@capacitor/android": "^5.0.0",
    "@capacitor/app": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/haptics": "^5.0.0",
    "@capacitor/keyboard": "^5.0.0",
    "@capacitor/share": "^5.0.0",
    "@capacitor/splash-screen": "^5.0.0",
    "@capacitor/status-bar": "^5.0.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "@wagmi/core": "^1.3.8",
    "@web3modal/ethereum": "^2.7.1",
    "@web3modal/html": "^2.7.1",
    "components": "*",
    "ethers": "^5.7.2",
    "firebase": "^10.1.0",
    "icons": "*",
    "ionicons": "7.1.0",
    "pinia": "^2.0.33",
    "send-intent": "^5.0.0",
    "shared": "*",
    "viem": "^1.5.3",
    "vue": "^3.2.47",
    "vue-cup-avatar": "^0.1.9",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@capacitor/assets": "^2.0.4",
    "@capacitor/cli": "^5.0.0",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "cypress": "^12.9.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "jest": "^29.5.0",
    "sass": "^1.60.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  },
  "browser": {
    "stream": false
  },
  "disabedDeps": {
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-node-stdlib-browser": "^0.2.1",
    "vite-plugin-windicss": "^1.8.10",
    "node-stdlib-browser": "^1.2.0"
  }
}
