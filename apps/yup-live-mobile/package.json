{
  "name": "yup-live-mobile",
  "version": "0.0.1",
  "private": true,
  "description": "Ionic Vue Yup Live Mobile",
  "scripts": {
    "dev": "vite",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore --fix src&&prettier . -w -u",
    "build": "vue-tsc --noEmit && vite build",
    "android": "ionic cap run android -l --external --host=4875.flashsoft.eu --port=4875 --livereload-url=https://4875.flashsoft.eu",
    "android-build": "yarn build && ionic capacitor sync android --prod --no-build",
    "android-open": "ionic capacitor open android",
    "android-res": "cordova-res android --skip-config --copy",
    "upgrade": "yarn-upgrade-all"
  },
  "dependencies": {
    "@capacitor/android": "^4.7.1",
    "@capacitor/app": "^4.1.1",
    "@capacitor/core": "^4.7.1",
    "@capacitor/haptics": "^4.1.0",
    "@capacitor/keyboard": "^4.1.1",
    "@capacitor/share": "^4.1.1",
    "@capacitor/splash-screen": "^4.2.0",
    "@capacitor/status-bar": "^4.1.1",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ionic/storage": "^3.0.6",
    "@ionic/vue": "^7.0.0",
    "@ionic/vue-router": "^7.0.0",
    "buffer": "^6.0.3",
    "components": "*",
    "ethers": "^5.7.2",
    "icons": "*",
    "ionicons": "^7.1.0",
    "omit-deep": "^0.3.0",
    "pinia": "^2.0.33",
    "send-intent": "^3.0.12",
    "shared": "*",
    "turbo": "^1.8.8",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "web3modal": "^1.9.12"
  },
  "devDependencies": {
    "@capacitor/cli": "^4.7.1",
    "@types/jest": "^29.5.0",
    "@types/omit-deep": "^0.3.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/cli-service": "^5.0.8",
    "cypress": "^12.9.0",
    "eslint": "^8.37.0",
    "eslint-plugin-vue": "^9.10.0",
    "jest": "^29.5.0",
    "sass": "^1.60.0",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-node-stdlib-browser": "^0.2.1",
    "vite-plugin-windicss": "^1.8.10",
    "vue-tsc": "^1.2.0"
  },
  "browser": {
    "stream": false
  }
}
