{
  "name": "yup-live-turbo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build:all": "turbo run build",
    "build": "turbo run build --filter=yup-live",
    "dev": "turbo run dev --filter=yup-live",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "turbo": "^1.11.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
